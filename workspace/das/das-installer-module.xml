<?xml version="1.0" encoding="UTF-8"?>

<module>
	<id>com.ils.installer</id>
	<name>DASInstaller</name>
	<description>DOW AgroSciences - Robotics KPI Installer - Phase II</description>
	<license></license>
	<version>10.42.0.4</version>
	<freemodule>true</freemodule>
	<requiredignitionversion>7.9.1</requiredignitionversion>
	<requiredframeworkversion>8</requiredframeworkversion>

	<!-- Load in our jars for each scope -->
	<export scope="G">ils-common.jar</export>
	<jar scope="G">ai-gateway.jar</jar>
	<!-- Tell the Gateway and/or Designer where to find the hooks -->
	<hook scope="G">com.ils.ai.gateway.ApplicationInstallerGatewayHook</hook>
</module>

